<div class="row">
  <div class="col s12">
    <h1>Pedido</h1>

    <!-- objective status -->
    <h3>Estado del objetivo</h3>
    <div class="progress_bars">
      <strong>Calorías</strong>
      <div class="progress grey">
        <div class="determinate yellow" style="width: 20%"></div>
      </div>

      <strong>Proteínas</strong>
      <div class="progress grey">
        <div class="determinate light-green" style="width: 60%"></div>
      </div>

      <strong>Grasas</strong>
      <div class="progress grey">
        <div class="determinate light-green" style="width: 40%"></div>
      </div>

      <strong>Carbohidratos</strong>
      <div class="progress grey">
        <div class="determinate red" style="width: 95%"></div>
      </div>
    </div>
    <!-- objective status -->
    
    <div class="order">
      <ul class="collection">
        <li class="collection-item">
          <h3>Comida #1</h3>
          <table class="highlight centered responsive-table">
            <thead>
              <tr>
                <th data-field="name">Producto</th>
                <th data-field="calories">Calorías</th>
                <th data-field="protein">Proteínas</th>
                <th data-field="fat">Grasas</th>
                <th data-field="carbs">Carbohidratos</th>
                <th data-field="price">Cantidad</th>
                <th data-field="edit">Modificar</th>
                <th data-field="delete">Eliminar</th>
                <th data-field="price">Precio</th>
