            </thead>
            <tbody>
              <tr>
                <td>Pechuga de pollo</td>
                <td>60</td>
                <td>20</td>
                <td>3</td>
                <td>0</td>
                <td>100 gr</td>
                <td>
                  <a class="waves-effect waves-light btn light-green">
                    <i class="material-icons">add</i>
                  </a>
                  <a class="waves-effect waves-light btn grey">
                    <i class="material-icons">remove</i>
                  </a>
                </td>
                <td>
                  <a class="waves-effect waves-light btn red">
                    <i class="material-icons">delete</i>
                  </a>
                </td>
                <td>0,5 €</td>
              </tr>
            </tbody>
          </table>
          <a class="waves-effect waves-light btn lime">
            <i class="material-icons left">add</i>
            Añadir Producto
          </a>
        </li>
      </ul>
      <a class="waves-effect waves-light btn light-green">
        <i class="material-icons left">add</i>
        Nueva Comida
      </a>
    </div>
    <a class="waves-effect waves-light btn green right">
      <i class="material-icons left">add</i>
      Comprar
    </a>
  </div>
</div>
